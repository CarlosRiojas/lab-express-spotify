<div class="card" style="width: 18rem;">
  <img src="{{images.0.url}}" class="card-img-top" alt="albums">
  <div class="card-body">
    <h5 class="card-title">{{name}}</h5>
    <a href="albums/{{artistId}}/tracks/{{id}}" class="btn my-2 rounded-pill">Peek tracks</a>
  </div>
</div>

<div class="container">
    <div class="row row-cols-3">
        {{#each data}}
        {{> albumCard this}}
        {{/each}}
    </div>
</div>